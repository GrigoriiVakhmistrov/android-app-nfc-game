<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.tzadmin.nfc_reader_writer.MainValidationActivity">
    <!--android:background="@drawable/mainback"-->

    <Button
        android:id="@+id/btnRefresh1"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:background="@drawable/ic_sync_ok"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintHorizontal_bias="0.946"
        app:layout_constraintVertical_bias="0.096"
        />

    <Button
        android:id="@+id/btnItsOnlyValidation"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:background="@drawable/main_validation"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.747" />

</android.support.constraint.ConstraintLayout>
